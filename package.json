  "scripts": {
    "start": "react-scripts start",
    "docker:build": "docker build -t recipemanager:latest .",
    "docker": "docker run -it --rm -v $(pwd):/app -v /app/node_modules -p 3000:3000  -e CHOKIDAR_USEPOLLING=true recipemanager:latest",
